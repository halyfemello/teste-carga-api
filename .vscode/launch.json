  "version": "0.2.0",
  "configurations": [
    {
      "name": "Teste Carga API",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-api",
      "program": "${workspaceFolder}/TesteCarga.Api/bin/Debug/net8.0/TesteCarga.Api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/TesteCarga.Api",
      "stopAtEntry": false,
      "internalConsoleOptions": "neverOpen",
      "launchBrowser": {
        "enabled": false
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://localhost:8084"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    }
  ]
}
